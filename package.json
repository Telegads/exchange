    "i18next": "^22.0.6",
    "jest-mock-extended": "^2.0.4",
    "js-file-download": "^0.4.12",
    "json2csv": "^5.0.7",
    "lodash": "^4.17.21",
    "next": "12.0.10",
    "next-auth": "^4.10.3",
    "next-i18next": "^13.0.0",
    "node-cron": "^3.0.2",
    "npm": "^9.1.3",
    "react": "17.0.2",
    "react-bootstrap": "^2.6.0",
    "react-dom": "17.0.2",
    "react-highlight-words": "^0.18.0",
    "react-hook-form": "^7.40.0",
    "react-i18next": "^12.0.0",
    "react-icons": "^4.6.0",
    "react-markdown": "8.0.0",
    "react-portal": "^4.2.2",
    "react-slider": "^2.0.4",
