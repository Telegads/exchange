{
  "name": "telegads-exchange",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start:bg-task": "ts-node ./src/backgroundTasks/start.ts",
    "start": "concurrently \"npm run start:bg-task\"  \"next start\" ",
    "db:run": "docker-compose up -d --build",
    "image:build": "docker build -f ./deploy/Dockerfile -t peteranikitin/telegads-exchange:latest  . ",
    "image:push": "docker push peteranikitin/telegads-exchange:latest   ",
    "release": "npm run image:build && npm run image:push",
    "precommit-hook": "npm run check-types && lint-staged",
    "check-types": "tsc --noEmit",
    "check-scripts": "eslint --ext .tsx,.ts,.js src",
    "format-files": "prettier --write .",
    "test": "jest -i",
    "test-dev": "jest --watch --coverage",
    "prepare": "husky install",
    "prisma-generate": "prisma generate",
    "postinstall": "npm run prisma-generate",
    "prisma-migrate": "prisma migrate deploy"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=production eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
